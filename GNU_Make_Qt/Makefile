##### build rules ############################################################
make_first: sub-lib-make_first-ordered sub-src-make_first-ordered

sub-lib-make_first-ordered:
	@test -d lib/ || mkdir -p lib/
	cd lib/ && $(MAKE) -f Makefile 

sub-src-make_first-ordered: sub-lib-make_first-ordered
	@test -d src/ || mkdir -p src/
	cd src/ && $(MAKE) -f Makefile 
##--------------------------------------------------------------------------##
clean: sub-lib-clean-ordered sub-src-clean-ordered
	rm -f *~

sub-lib-clean-ordered:
	@test -d lib/ || mkdir -p lib/
	cd lib/ && $(MAKE) -f Makefile clean

sub-src-clean-ordered: sub-lib-clean-ordered
	@test -d src/ || mkdir -p src/
	cd src/ && $(MAKE) -f Makefile clean

